{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-commonjs": "off",
        "import/prefer-default-export": "off",
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "_id",
                    "_message",
                    "_doc"
                ]
            }
        ]
    }
}